// list of printable characters in ASCII table, with some additions
globalvar String ascii = " !\"#$%&'()*+,-./0123456789:;<=>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\\]^_`abcdefghijklmnopqrstuvwxyz{|}~";

String DecodeAsciiCharacter(Number number): 
  ascii.CharAt(number - 1);

String DecodeAsciiString(in Number[] numbers) {
  String string = "";
  foreach(Number num in numbers) {
    string = $"{string}{DecodeAsciiCharacter(num)}";
  }
  return string;
}
